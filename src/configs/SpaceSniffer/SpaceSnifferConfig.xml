    <fileGroups>
        <fileGroup>
            <name>Video/Movies</name>
            <extensions>qt;mov;mpeg;mpg;mpe;m1s;mp4;avi;mts;vob;wmv;rm;3gp;mkv</extensions>
            <color>0x00F77909</color>
        </fileGroup>
        <fileGroup>
            <name>Executables/System files</name>
            <extensions>exe;dll;sys;ini;msi;log</extensions>
            <color>0x002F2F2F</color>
        </fileGroup>
        <fileGroup>
            <name>Archives/Disk images</name>
            <extensions>zip;7zip;rar;iso</extensions>
            <color>0x006D43BC</color>
        </fileGroup>
        <fileGroup>
            <name>Pictures/Images</name>
            <extensions>jpg;jpeg;gif;png;tif;tiff;nef;raw;rw2;cr2;bmp;psd;tga;exr;afphoto</extensions>
            <color>0x00FDEDCC</color>
        </fileGroup>
        <fileGroup>
            <name>Audio/Music</name>
            <extensions>mp2;mp3;wav;ogg;aif;alp;au;3gp;ac3;amr;aac;m4a;wma;aiff;flac;m4a;voc</extensions>
            <color>0x008DEE46</color>
        </fileGroup>
        <fileGroup>
            <name>Cache</name>
            <extensions>geo;bgeo;bgeo.sc;obj;ply;sim</extensions>
            <color>0x0057D8FD</color>
        </fileGroup>
        <fileGroup>
            <name>CG</name>
            <extensions>spp;hip;hiplc;</extensions>
            <color>0x0046A4F9</color>
        </fileGroup>
        <fileGroup>
            <name>New file group</name>
            <extensions>fbx;ztl</extensions>
            <color>0x005778FD</color>
        </fileGroup>
    </fileGroups>
</SpaceSnifferConfig>
