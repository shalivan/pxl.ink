<?xml version="1.0" encoding="UTF-8" ?>
<SpaceSnifferConfig>
    <refreshDelayMs>50</refreshDelayMs>
    <zoomTimeMs>5</zoomTimeMs>
    <minElementSizePx>9</minElementSizePx>
    <showFreeSpace>true</showFreeSpace>
    <showUnknownSpace>true</showUnknownSpace>
    <initialStyle>fileClasses</initialStyle>
    <showFileTooltipDateTime>false</showFileTooltipDateTime>
    <showFileTooltipAge>true</showFileTooltipAge>
    <useRealZoom>false</useRealZoom>
    <useDynaSmooth>true</useDynaSmooth>
    <useSorting>true</useSorting>
    <scanADS>false</scanADS>
    <watchDog>true</watchDog>
    <logPopup>false</logPopup>
    <logWarning>false</logWarning>
    <flashWindow>true</flashWindow>
    <keepTooltip>true</keepTooltip>
    <proportions>15</proportions>
    <initialDetailLevel>15</initialDetailLevel>
    <fontSize>7</fontSize>
    <colors>
        <driveColor>0x0000ACFF</driveColor>
        <folderColor>0x008CA8AE</folderColor>
        <fileColor>0x00FFCC99</fileColor>
        <freeColor>0x0036E9AA</freeColor>
        <unknownColor>0x00808080</unknownColor>
        <levelContrast>9</levelContrast>
        <borderContrast>28</borderContrast>
        <mouseHover>true</mouseHover>
        <mouseTrail>true</mouseTrail>
        <hilightShadow>true</hilightShadow>
        <hilightHaloLevels>1</hilightHaloLevels>
    </colors>
    <exportConfigs>
